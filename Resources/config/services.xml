<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Vaderlab\EAV\Core\Service\DataType\DataTypeProvider">
            <argument type="collection">
                <argument key="integer">Vaderlab\EAV\Core\Entity\ValueType\ValueInteger</argument>
                <argument key="float">Vaderlab\EAV\Core\Entity\ValueType\ValueFloat</argument>
                <argument key="boolean">Vaderlab\EAV\Core\Entity\ValueType\ValueBoolean</argument>
                <argument key="string">Vaderlab\EAV\Core\Entity\ValueType\ValueString</argument>
                <argument key="text">Vaderlab\EAV\Core\Entity\ValueType\ValueText</argument>
            </argument>
        </service>
    </services>
</container>